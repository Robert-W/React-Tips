{
  "name": "react-boilerplate",
  "version": "0.1.0",
  "description": "Tips and tricks for applications using React, Flux, and ES6 with Esri's JavaScript API",
  "main": "gulpfile.js",
  "scripts": {
    "start": "gulp build & npm run babel -- -w & gulp serve",
    "babel": "babel src --out-dir build",
    "dist": "gulp dist; babel src --out-dir ./; node scripts/prerender",
    "postinstall": "bower install"
  },
  "keywords": [
    "esri",
    "flux",
    "react",
    "javascript",
    "arcgis"
  ],
  "author": "Robert-W",
  "license": "MIT",
  "dependencies": {
    "babel-cli": "^6.3.17",
    "babel-core": "^6.3.26",
    "babel-plugin-transform-es2015-modules-amd": "^6.3.13",
    "babel-polyfill": "^6.3.14",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-stage-0": "^6.3.13",
    "bower": "^1.7.2",
    "browser-sync": "^2.11.0",
    "cheerio": "^0.19.0",
    "gulp": "^3.9.0",
    "gulp-autoprefixer": "^3.1.0",
    "gulp-jade": "^1.1.0",
    "gulp-stylus": "^2.2.0",
    "jstransformer-stylus": "^1.0.0",
    "madge": "^0.5.3",
    "react": "^0.14.6",
    "react-dom": "^0.14.6",
    "requirejs": "^2.1.22"
  },
  "devDependencies": {
    "babel-eslint": "^5.0.0-beta6",
    "eslint": "^1.10.3",
    "eslint-plugin-react": "^3.14.0"
  }
}
