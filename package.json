    "babel-core": "^6.3.26",
    "babel-plugin-transform-es2015-modules-amd": "^6.3.13",
    "babel-polyfill": "^6.3.14",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-stage-0": "^6.3.13",
    "bower": "^1.7.2",
    "browser-sync": "^2.11.0",
    "gulp": "^3.9.0",
    "gulp-jade": "^1.1.0",
    "gulp-stylus": "^2.2.0",
    "jstransformer-stylus": "^1.0.0"
  }
}
