{
  "name": "react-boilerplate",
  "version": "0.1.0",
  "description": "Tips and tricks for applications using React, Flux, and ES6 with Esri's JavaScript API",
  "main": "gulpfile.js",
  "scripts": {
    "start": "gulp build & npm run babel -- -w & gulp serve",
    "babel": "babel src --out-dir build",
    "postinstall": "bower install"
  },
